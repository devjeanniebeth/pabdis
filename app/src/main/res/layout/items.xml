<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:orientation="vertical">



        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="ID: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:visibility="invisible"/>



                <TextView
                    android:id="@+id/txtViewID"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:visibility="invisible"/>


            </TableRow>
        </TableLayout>

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Owner ID: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtViewOwner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>


        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Respondent Name: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>

                <TextView
                    android:id="@+id/txtViewFullR"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="Date of Survey"
                    android:textColor="#050505"
                    android:textSize="18sp" />

            </TableRow>
        </TableLayout>






        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Members: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtContact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>



        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="House Number: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>

                <TextView
                    android:id="@+id/txtHouse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>



        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Municipality: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtMuni"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>



        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Barangay: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtBrgy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>



        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Latitude: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtLati"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>


        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Longitude: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>
                <TextView
                    android:id="@+id/txtLongi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />

            </TableRow>
        </TableLayout>


        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="0,1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:layout_column="0"
                    android:text="Date Surveyed: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#050505"
                    android:textSize="18sp"/>


                <TextView
                    android:id="@+id/txtCreatedat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="QRCode"
                    android:textColor="#050505"
                    android:textSize="18sp" />


            </TableRow>
        </TableLayout>



    </LinearLayout>



</android.support.constraint.ConstraintLayout>